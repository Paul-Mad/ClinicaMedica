<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ClinicaMedica</display-name>
  <welcome-file-list>
    <welcome-file>menu_administracao.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>servletUsuarioInserir</servlet-name>
    <servlet-class>servlet.inserirUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletUsuarioInserir</servlet-name>
    <url-pattern>/inserirUsuario</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>servletUsuarioPesquisar</servlet-name>
    <servlet-class>servlet.pesquisarUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletUsuarioPesquisar</servlet-name>
    <url-pattern>/pesquisarUsuario</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>servletUsuarioAlterar</servlet-name>
    <servlet-class>servlet.atualizarUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletUsuarioAlterar</servlet-name>
    <url-pattern>/atualizarUsuario</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>servletUsuarioExcluir</servlet-name>
    <servlet-class>servlet.excluirUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletUsuarioExcluir</servlet-name>
    <url-pattern>/excluirUsuario</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>